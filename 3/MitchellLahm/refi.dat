molecule mol {
  0 1
  O
  H 1 0.95
  H 1 0.95 2 104.5
}

set basis         sto-3g
set e_convergence 10
set scf_type      pk
set maxiter       50

basisname = psi4.core.get_global_option("BASIS")
basisset  = psi4.core.BasisSet.build(mol, "BASIS", basisname)
mints     = psi4.core.MintsHelper(basisset)

# call RHF code
import sys
sys.path.insert(0, '.')
from RHF import RHF
rhf = RHF(mol, mints)
rhf.compute_energy()
