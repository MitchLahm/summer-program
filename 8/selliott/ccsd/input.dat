#H2O

molecule mol {
   0 1
   O 
   H 1 1.1
   H 1 1.1  2  104.0
   }

set {
    reference uhf
    basis sto-3g
    e_convergence 12
	MAXITER 100
    scf_type pk
    }

# initialize molecule object, integrals library
basisname = psi4.get_global_option("BASIS")
basisset  = psi4.BasisSet.pyconstruct_orbital(mol, "BASIS", basisname)
mints     = psi4.MintsHelper(basisset)
mol = psi4.get_active_molecule()

import sys
sys.path.insert(0,"../../../5/avcopan/")

from uhf import UHF
uhf = UHF(mol,mints)
uhf.compute_energy()

# call CCD code here
from ccsd import CCSD
ccsd = CCSD(uhf)
print ccsd.corr_energy()

